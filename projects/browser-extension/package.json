{
  "name": "heads-up-browser-extension",
  "version": "1.0.0",
  "description": "View your `<head>` data for easier developing.",
  "main": "devtools.js",
  "scripts": {
    "predev": "npm run prebuild",
    "dev": "run-p dev:* devtools",
    "dev:app": "webpack --watch",
    "dev:examples": "serve examples/",
    "prebuild-netlify": "cd ../rich-previews && npm i && npm run build && cd ../browser-extension && npm run prebuild",
    "prebuild": "rimraf ./dist && mkdir -p ./dist/previews && cp -R ../rich-previews/dist/** ./dist/previews",
    "build-netlify": "npm run prebuild-netlify && npm run build",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "start": "npm run dev",
    "devtools": "vue-devtools",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint"
  },
  "author": "De Voorhoede",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voorhoede/heads-up/issues"
  },
  "homepage": "https://github.com/voorhoede/heads-up#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "debounce": "^1.2.0",
    "robots-txt-parser": "^1.0.1",
    "v-tooltip": "^2.0.3",
    "vue": "^2.6.12",
    "vue-router": "^3.4.6",
    "vuex": "^3.5.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "buble": "^0.20.0",
    "buble-loader": "^0.5.1",
    "copy-webpack-plugin": "^5.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-vue": "^7.0.1",
    "file-loader": "^6.1.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "serialize-javascript": "^4.0.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.3",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.0.0",
    "webpack-merge": "^5.2.0"
  },
  "optionalDependencies": {
    "@vue/devtools": "^5.3.3",
    "serve": "^11.3.2"
  }
}
