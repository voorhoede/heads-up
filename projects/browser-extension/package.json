{
  "name": "heads-up-browser-extension",
  "version": "1.0.0",
  "description": "View your `<head>` data for easier developing.",
  "author": "De Voorhoede",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voorhoede/heads-up/issues"
  },
  "homepage": "https://github.com/voorhoede/heads-up#readme",
  "dependencies": {
    "debounce": "^1.2.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0"
  },
  "scripts": {
    "postinstall": "cd ./vue-app && npm i",
    "predev": "npm run prebuild",
    "dev:wrapper": "webpack --watch",
    "dev:vue-app": "cd ./vue-app && npm run dev",
    "dev": "run-p dev:*",
    "prebuild": "rimraf ./dist && mkdir -p ./dist/previews && cp -R ../rich-previews/dist/** ./dist/previews",
    "build:wrapper": "webpack",
    "build:vue-app": "cd ./vue-app && npm run build",
    "build": "cross-env NODE_ENV=production run-p build:*",
    "prebuild-netlify": "npm run postinstall && cd ../rich-previews && npm i && npm run build && cd ../@shared && npm i",
    "build-netlify": "npm run build",
    "start": "npm run dev",
    "lint": ""
  }
}
