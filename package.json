{
  "name": "heads-up",
  "version": "0.2.0",
  "description": "View your `<head>` data for easier developing.",
  "main": "devtools.js",
  "scripts": {
    "predev": "npm run prebuild",
    "dev": "run-p dev:* devtools",
    "dev:app": "webpack --watch --hide-modules",
    "dev:examples": "serve examples/",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "devtools": "vue-devtools",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voorhoede/heads-up.git"
  },
  "author": "De Voorhoede",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voorhoede/heads-up/issues"
  },
  "homepage": "https://github.com/voorhoede/heads-up#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "debounce": "^1.2.0",
    "v-tooltip": "^2.0.3",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@vue/devtools": "^5.3.3",
    "babel-eslint": "^10.1.0",
    "buble": "^0.19.8",
    "buble-loader": "^0.5.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.2.1",
    "husky": "3.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^4.1.0",
    "vue-loader": "^15.9.1",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "optionalDependencies": {
    "serve": "^11.3.0"
  }
}
