{
  "name": "heads-up",
  "version": "1.0.0",
  "description": "View your `<head>` data for easier developing.",
  "main": "devtools.js",
  "scripts": {
    "dev": "npm run prebuild && webpack --watch --hide-modules & npm run devtools",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "devtools": "vue-devtools",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voorhoede/heads-up.git"
  },
  "author": "De Voorhoede",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voorhoede/heads-up/issues"
  },
  "homepage": "https://github.com/voorhoede/heads-up#readme",
  "devDependencies": {
    "@vue/devtools": "^5.1.1",
    "babel-eslint": "^10.0.3",
    "buble": "^0.19.8",
    "buble-loader": "^0.5.1",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.3",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "vue-loader": "^15.7.1",
    "vue-svg-loader": "^0.12.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "debounce": "^1.2.0",
    "v-tooltip": "^2.0.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.2",
    "vuex": "^3.1.1"
  }
}
